{
  "name": "ziti-electron-fetch",
  "version": "2.6.24",
  "description": "A module that intercepts all window.fetch calls in an Electron renderer process and routes them over a Ziti network",
  "main": "lib/index.js",
  "files": [
    "lib/index.js",
    "lib/index.mjs",
    "lib/index.es.js",
    "browser.js"
  ],
  "scripts": {
    "build": "cross-env BABEL_ENV=rollup rollup -c",
    "prepare": "npm run build",
    "testx": "cross-env BABEL_ENV=test mocha --require @babel/register --throw-deprecation test/test.js",
    "test:fetch": "cross-env BABEL_ENV=test mocha --require @babel/register --throw-deprecation test/fetch-test.js",
    "test": "echo \"Error: no test specified\"",
    "report": "cross-env BABEL_ENV=coverage nyc --reporter lcov --reporter text mocha -R spec test/test.js",
    "coverage": "cross-env BABEL_ENV=coverage nyc --reporter json --reporter text mocha -R spec test/test.js && codecov -f coverage/coverage-final.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/netfoundry/node-fetch.git"
  },
  "keywords": [
    "ziti",
    "nodejs",
    "electron",
    "fetch",
    "http",
    "promise"
  ],
  "engines": {
    "node": ">=11.0.0"
  },
  "author": {
    "name": "NetFoundry",
    "url": "http://netfoundry.io"
  },
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "bugs": {
    "url": "https://github.com/netfoundry/ziti-electron-fetch/issues"
  },
  "homepage": "https://github.com/netfoundry/ziti-electron-fetch",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-external-helpers": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "@ungap/url-search-params": "^0.1.2",
    "abort-controller": "^1.1.0",
    "abortcontroller-polyfill": "^1.3.0",
    "aws-sdk": "^2.614.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^7.1.1",
    "chai-iterator": "^1.1.1",
    "chai-string": "~1.3.0",
    "codecov": "^3.6.5",
    "cross-env": "^5.2.0",
    "is-builtin-module": "^1.0.0",
    "mocha": "^5.0.0",
    "nyc": "11.9.0",
    "parted": "^0.1.1",
    "promise": "^8.0.3",
    "resumer": "0.0.0",
    "rollup": "^0.63.4",
    "rollup-plugin-babel": "^4.3.3",
    "string-to-arraybuffer": "^1.0.2",
    "whatwg-url": "^5.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "async-mutex": "^0.1.4",
    "asynckit": "^0.4.0",
    "cache-manager": "^2.11.1",
    "combined-stream": "^1.0.8",
    "debug": "^4.1.1",
    "electron": "^5.0.11",
    "electron-log": "^4.1.0",
    "encoding": "^0.1.12",
    "mime-types": "^2.1.27",
    "uuid": "^8.0.0",
    "ziti-sdk-nodejs": "0.1.33"
  }
}
